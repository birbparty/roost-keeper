            "expr": "up{job=\"roost-keeper-operator\"}",
            "format": "table",
            "instant": true,
            "interval": "",
            "legendFormat": "",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus", 
              "uid": "${datasource}"
            },
            "expr": "avg(roost_keeper_health_check_success_total / roost_keeper_health_check_total) > 0.95",
            "format": "table",
            "instant": true,
            "interval": "",
            "legendFormat": "",
            "refId": "B"
          }
        ],
        "title": "üö¶ System Health Status",
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "job": true
