              "indexByName": {},
              "includeByName": {},
              "renameByName": {
                "Value #A": "Operator",
                "Value #B": "Health Checks",
                "instance": "Component"
              }
            }
          }
        ],
        "type": "table"
      },
      {
        "datasource": {
