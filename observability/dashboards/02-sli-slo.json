          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true
              },
              "indexByName": {},
              "includeByName": {},
              "renameByName": {
                "Value #A": "Availability (99.5% target)",
                "Value #B": "Creation Latency (30s target)",
                "Value #C": "Health Check Latency (1s target)",
                "Value #D": "Error Rate (1% target)"
              }
            }
          }
        ],
        "type": "table"
      },
