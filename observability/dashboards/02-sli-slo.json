        "description": "Current SLO status across all service level objectives",
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "custom": {
              "align": "auto",
              "displayMode": "color-background",
              "inspect": false
            },
            "mappings": [
              {
                "options": {
                  "pattern": "^[0-9]\\.[0-9]+$",
                  "result": {
                    "color": "green",
                    "index": 0
                  }
                },
                "type": "regex"
              }
            ],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.99
                },
                {
                  "color": "green",
                  "value": 0.995
                }
              ]
            },
            "unit": "percentunit"
          },
          "overrides": []
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 1
        },
        "id": 2,
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": false,
              "displayName": "SLO"
            }
          ]
        },
        "pluginVersion": "9.0.0",
        "targets": [
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "expr": "sum(rate(roost_keeper_health_check_success_total[30d])) / sum(rate(roost_keeper_health_check_total[30d]))",
            "format": "table",
            "instant": true,
            "interval": "",
            "legendFormat": "Availability",
            "refId": "A"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "expr": "(sum(rate(roost_keeper_helm_install_duration_seconds_sum[7d])) / sum(rate(roost_keeper_helm_install_duration_seconds_count[7d]))) < 30",
            "format": "table",
            "instant": true,
            "interval": "",
            "legendFormat": "Creation Latency",
            "refId": "B"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "expr": "(sum(rate(roost_keeper_health_check_duration_seconds_sum[7d])) / sum(rate(roost_keeper_health_check_duration_seconds_count[7d]))) < 1",
            "format": "table",
            "instant": true,
            "interval": "",
            "legendFormat": "Health Check Latency",
            "refId": "C"
          },
          {
            "datasource": {
              "type": "prometheus",
              "uid": "${datasource}"
            },
            "expr": "sum(rate(roost_keeper_reconcile_errors_total[7d])) / sum(rate(roost_keeper_reconcile_total[7d]))",
            "format": "table",
            "instant": true,
            "interval": "",
            "legendFormat": "Error Rate",
            "refId": "D"
          }
